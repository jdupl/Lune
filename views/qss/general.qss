QWidget{
	color: #eee;
}
QWidget#mainwindow {
	background: #444;
}


QWidget#nav > QPushButton {
	color: gray;
	background: transparent;
	border-width: 0px;
	font-size: 25px;
	margin: 0;
	margin-right: 20px;
}
QWidget#nav > QPushButton:hover {
	color: lightgray;
}

QWidget#wcontrols > QPushButton {
	color: gray;
	background: transparent;
	border-width: 0px;
	height: 18px;
	width: 18px;
	font-size: 30px;
	font-family: luneicons;
	margin: 0;
}
QWidget#wcontrols > QPushButton:hover {
	color: lightgray;
}


QListWidget {
	background: #333;
	padding: 4px;
	border-style: outset;
	border-width: 0px;
}
QListWidget::item {
	color: #EEE;
}
QListWidget::item:selected {
	background: #888888;
	border-style: outset;
	border-width: 1px;
	border-color: #EB008B;
}
QListWidget::item:hover {
	background: #666666;
}
